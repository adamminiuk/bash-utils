function stash {
	echo 'from stash';
	git archive --format zip --output ../Widget/ipla_samsung.zip `git stash create`
}

function head {
	echo 'from HEAD';
	git archive --format zip --output ../Widget/ipla_samsung.zip HEAD
}

if [ -n "$(git status --untracked-files=no -s)" ]; then 
  stash
else 
  head
fi